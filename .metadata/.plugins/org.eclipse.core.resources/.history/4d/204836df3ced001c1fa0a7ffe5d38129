

create table once(
num number(5) primary key,
pass varchar2(30),
userid varchar2(30),
title varchar2(50),
content varchar2(1000),
readcount number(4) default 0,
writedate date default sysdate

)


create table people(
name varchar2(30),
userid varchar2(30),
pwd varchar2(30),
phone varchar2(30)
primary key(userid)
);
